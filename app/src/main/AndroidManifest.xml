<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kasirpintartest">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.KasirPintarTest">
        <activity android:name=".ui.transactionsuccess.TransactionSuccessActivity"></activity>
        <activity android:name=".ui.checkout.CheckoutActivity" />
        <activity android:name=".ui.home.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.order.OrderActivity" />
        <activity android:name=".ui.transaction.TransactionActivity" />
        <activity
            android:name=".ui.addupdate.AddUpdateActivity"
            android:parentActivityName=".ui.main.MainActivity" />
        <activity
            android:name=".ui.main.MainActivity"
            android:launchMode="singleTop" />
    </application>

</manifest>